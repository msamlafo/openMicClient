Mic/openMic-site/openMicClient/open-mic/src/Components/Poem/UpdatePoem.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/FormInput.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/TypeConfig.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Poem/ViewAllPoems.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/PagesPagination.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/Paginate.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/Like.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Profile/ViewProfile.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Profile/UpdateProfile.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Profile/ProfileForm.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/ImageInput.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/FormGroupButton.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Comments/ViewAllComment.tsx",["139"],"import React, { Component } from 'react';\nimport { Comment,CommentDefaultObject } from '../../Common/TypeConfig';\nimport CommentForm from '../Comments/CommentForm';\nimport { BASE_API_URL, getLoginToken } from '../../Common/Environment';\nimport CommentList from './CommentsList';\nimport EditComment from './EditComment';\nimport DeleteComment from './DeleteComment';\n\ntype ViewAllCommentProps = {\n  comments: Comment[];\n  poetryId: number;\n  onReload: Function;\n};\n\ntype ViewAllCommentState = {\n  selectedComment: Comment;\n  showModal: boolean;\n};\n\ntype commentFormFieldName = 'comment';\n\nclass ViewAllComment extends Component<\n  ViewAllCommentProps,\n  ViewAllCommentState\n> {\n  constructor(props: ViewAllCommentProps) {\n    super(props);\n    this.state = {\n      selectedComment: CommentDefaultObject,\n      showModal: false,\n    };\n  }\n\n  handleChange = (\n    event: React.FormEvent<HTMLSelectElement | HTMLInputElement>\n  ): void => {\n    const value = event.currentTarget.value;\n    const name = event.currentTarget.name as commentFormFieldName;\n    const selectedComment = { ...this.state.selectedComment };\n    selectedComment[name] = value;\n    this.setState({ selectedComment });\n  };\n\n  handleCreateSubmit = async (event: React.SyntheticEvent) => {\n    event.preventDefault();\n    const { selectedComment: comment } = this.state;\n    const { poetryId } = this.props;\n    console.log('form submitted');\n    const endpoint = `${BASE_API_URL}/comment`;\n    const fetchOptions = {\n      method: 'POST',\n      body: JSON.stringify({ comment: comment.comment, poetryId }),\n      headers: new Headers({\n        'Content-Type': 'application/json',\n        Authorization: getLoginToken(),\n      }),\n    };\n\n    const postResult = await fetch(endpoint, fetchOptions);\n\n    console.log(postResult);\n\n    if (postResult.status === 200) {\n      this.setState({\n        selectedComment: CommentDefaultObject,\n      });\n      this.props.onReload();\n    }\n  };\n\n  handleUpdateSubmit = async (event: React.SyntheticEvent) => {\n    event.preventDefault();\n    const { selectedComment: comment } = this.state;\n    const { poetryId } = this.props;\n    console.log('form submitted');\n    const endpoint = `${BASE_API_URL}/comment/${comment.id}`;\n    const fetchOptions = {\n      method: 'PUT',\n      body: JSON.stringify({ comment: comment.comment, poetryId }),\n      headers: new Headers({\n        'Content-Type': 'application/json',\n        Authorization: getLoginToken(),\n      }),\n    };\n\n    const postResult = await fetch(endpoint, fetchOptions);\n\n    console.log(postResult);\n\n    if (postResult.status === 200) {\n      this.setState({\n        selectedComment: { id: 0, comment: '', poetryId: 0, createdAt: '' },\n      });\n      this.handleEditModalToggle();\n      this.props.onReload();\n    }\n  };\n\n  handleDeleteSubmit = (event: React.SyntheticEvent) =>{\n    event.preventDefault();\n    const API_URL = `http://localhost:4000/comment/${this.state.selectedComment.id}`;\n    console.log('submit delete comment info', this.state);\n\n    fetch (`${API_URL}`, {\n        method: 'DELETE',\n        headers: new Headers({\n            'Content-Type' : 'application/json',\n            \"Authorization\" : localStorage.getItem('token') || \"\"\n        }),\n    })\n    .then((result) => result.json())\n    .then((response) => {\n        if (response.status === 200) {\n            console.log(response);\n        }\n    })\n    .catch((error) => console.log(error));\n}\n\n  handleEditClick =(comment:Comment) => {\n    this.setState({selectedComment:comment, showModal: !this.state.showModal});\n  }\n\n  handleEditModalToggle = () => {\n    this.setState({ showModal: !this.state.showModal });\n  };\n\n  \n  render() {\n    const { comments } = this.props;\n    console.log(comments);\n    return (\n      <div>\n        <CommentForm\n          comment={this.state.selectedComment}\n          onChange={this.handleChange}\n          onReload={this.props.onReload}\n          onSubmit={this.handleCreateSubmit}\n        />\n        <CommentList\n          comments={comments}\n          comment={this.state.selectedComment}\n          onToggle={this.handleEditModalToggle}\n          poetryId={this.props.poetryId}\n          onChange={this.handleChange}\n          showModal={this.state.showModal}\n          onEditClick={this.handleEditClick}\n        />\n        <EditComment\n          comment={this.state.selectedComment}\n          showModal={this.state.showModal}\n          onChange={this.handleChange}\n          onToggle={this.handleEditModalToggle}\n          onSubmit={this.handleUpdateSubmit}\n        />\n        {/* <DeleteComment onSubmit={(event) =>this.handleDeleteSubmit(event)}/> */}\n      </div>\n    );\n  }\n}\n\nexport default ViewAllComment;\n","/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Comments/DeleteComment.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Common/ListGroup.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Poem/PoemCategory.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Auth/NotFound.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src/Components/Comments/CommentForm.tsx",[],"/Users/rdarku/Desktop/ElevenFiftyProjects/javascriptAugust2020/javascriptLibrary/redBadge/openMic/openMic-site/openMicClient/open-mic/src